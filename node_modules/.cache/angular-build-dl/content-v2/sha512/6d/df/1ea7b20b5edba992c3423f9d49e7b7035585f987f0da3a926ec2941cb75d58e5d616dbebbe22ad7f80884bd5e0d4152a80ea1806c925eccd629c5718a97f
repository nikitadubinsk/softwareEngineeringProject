let WorkerOneRequestComponent = class WorkerOneRequestComponent {
    constructor() {
        this.editRequest = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        this.editFlag = false;
    }
    ngOnInit() {
    }
    edit(request) {
        this.editFlag = true;
        this.form = new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormGroup"]({
            status: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](request.status, [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].minLength(1)])
        });
        this.newRequest = request;
    }
    save() {
        this.editFlag = false;
        if (this.form.value.status == 'В работе') {
            this.newRequest.status = this.form.value.status;
            this.newRequest.emailWorker = localStorage['email'];
            delete this.newRequest.dateOfFinish;
        }
        if (this.form.value.status == 'Готово') {
            this.newRequest.status = this.form.value.status;
            this.newRequest.dateOfFinish = new Date();
        }
        this.editRequest.emit(this.newRequest);
    }
};
